<script>
// contar total de celulas
function contarItems() {
    var cantidad = jQuery('.jet-listing-grid__item').length;
    jQuery('.totalcels').text(cantidad);
}

jQuery(document).ready(function(){
    contarItems(); // Llamada inicial a la función para mostrar los resultados al cargar la página
    setInterval(contarItems, 5000); // Llamada a la función cada 5 segundos para actualizar los resultados
});
</script>
